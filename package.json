{
	"name": "exome",
	"version": "1.5.6",
	"description": "Proxy based store manager for deeply nested states",
	"main": "./dist/exome.js",
	"module": "./dist/exome.mjs",
	"types": "./dist/exome.d.ts",
	"scripts": {
		"build": "node ./scripts/build.mjs && npm run declarations",
		"dev": "npm run build -- --sourcemap --define:process.env.NODE_ENV=\\\"development\\\" --watch",
		"playground": "esbuild playground/index.tsx --outfile=playground/dist/index.js --bundle --format=esm --sourcemap --define:process.env.NODE_ENV=\\\"development\\\" && npx serve playground/dist",
		"lint": "rome ci src",
		"test": "uvu -r esbuild-register -i e2e",
		"e2e": "uvu -r esbuild-register -i src",
		"coverage": "c8 --check-coverage npm test",
		"declarations": "tsc --declarationDir dist --emitDeclarationOnly --declaration",
		"postbuild": "cat package.json | sed '/\\\"devDependencies\\\"/,/}/ d; /^$/d' | sed 's/\\.\\/dist\\//\\.\\//g' > ./dist/package.json && cp README.md dist && cp LICENSE dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Marcisbee/exome.git"
	},
	"keywords": [
		"store",
		"state",
		"state-manager",
		"proxy",
		"deep",
		"nested",
		"react",
		"preact",
		"vue",
		"lit",
		"rxjs",
		"svelte"
	],
	"author": "Marcis <marcisbergmanis@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Marcisbee/exome/issues"
	},
	"homepage": "https://github.com/Marcisbee/exome#readme",
	"devDependencies": {
		"@types/node": "^18.6.1",
		"@types/proxyquire": "^1.3.28",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"@types/sinon": "^10.0.13",
		"c8": "^7.12.0",
		"esbuild": "^0.17.10",
		"esbuild-register": "^3.4.2",
		"lit": "^2.2.8",
		"playwright": "^1.24.1",
		"preact": "^10.10.0",
		"proxyquire": "^2.1.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rome": "^12.0.0",
		"rxjs": "^7.0.0",
		"sinon": "^14.0.0",
		"typescript": "^5.0.4",
		"uvu": "^0.5.6",
		"vue": "^3.2.37"
	},
	"exports": {
		".": {
			"require": "./dist/exome.js",
			"import": "./dist/exome.mjs"
		},
		"./devtools": {
			"require": "./dist/devtools.js",
			"import": "./dist/devtools.mjs"
		},
		"./ghost": {
			"require": "./dist/ghost.js",
			"import": "./dist/ghost.mjs"
		},
		"./state": {
			"require": "./dist/state.js",
			"import": "./dist/state.mjs"
		},
		"./react": {
			"require": "./dist/react.js",
			"import": "./dist/react.mjs"
		},
		"./preact": {
			"require": "./dist/preact.js",
			"import": "./dist/preact.mjs"
		},
		"./vue": {
			"require": "./dist/vue.js",
			"import": "./dist/vue.mjs"
		},
		"./lit": {
			"require": "./dist/lit.js",
			"import": "./dist/lit.mjs"
		},
		"./rxjs": {
			"require": "./dist/rxjs.js",
			"import": "./dist/rxjs.mjs"
		},
		"./svelte": {
			"require": "./dist/svelte.js",
			"import": "./dist/svelte.mjs"
		}
	}
}
