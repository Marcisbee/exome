{
  "name": "exome",
  "version": "1.5.2",
  "description": "Proxy based store manager for deeply nested states",
  "main": "exome.js",
  "module": "exome.mjs",
  "types": "index.d.ts",
  "scripts": {
    "build": "node -r esbuild-register ./scripts/build.ts && npm run declarations",
    "dev": "npm run build -- --sourcemap --define:process.env.NODE_ENV=\\\"development\\\" --watch",
    "playground": "esbuild playground/index.tsx --outfile=playground/dist/index.js --bundle --format=esm --sourcemap --define:process.env.NODE_ENV=\\\"development\\\" && npx serve playground/dist",
    "lint": "rome check src",
    "test": "uvu -r esbuild-register -i e2e",
    "e2e": "uvu -r esbuild-register -i src",
    "coverage": "c8 --check-coverage npm test",
    "declarations": "tsc --declarationDir dist --emitDeclarationOnly --declaration",
    "postbuild": "cp package.json dist && cp README.md dist && cp LICENSE dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marcisbee/exome.git"
  },
  "keywords": [
    "store",
    "state",
    "state-manager",
    "proxy",
    "deep",
    "nested",
    "react",
    "preact",
    "vue",
    "lit",
    "rxjs",
    "svelte"
  ],
  "author": "Marcis <marcisbergmanis@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Marcisbee/exome/issues"
  },
  "homepage": "https://github.com/Marcisbee/exome#readme",
  "devDependencies": {
    "@types/node": "^18.6.1",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/sinon": "^10.0.13",
    "c8": "^7.12.0",
    "esbuild": "^0.17.10",
    "esbuild-register": "^3.4.2",
    "lit": "^2.2.8",
    "playwright": "^1.24.1",
    "preact": "^10.10.0",
    "proxyquire": "^2.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rome": "^11.0.0",
    "rxjs": "^7.0.0",
    "sinon": "^14.0.0",
    "typescript": "^4.7.4",
    "uvu": "^0.5.6",
    "vue": "^3.2.37"
  },
  "peerDependencies": {
    "lit": ">= 2.0.0",
    "preact": ">= 10.1.0",
    "react": ">= 16.8.0",
    "rxjs": ">= 6.0.0",
    "vue": ">= 3.0.0"
  },
  "peerDependenciesMeta": {
    "lit": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "preact": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "rxjs": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "require": "./exome.js",
      "import": "./exome.mjs"
    },
    "./devtools": {
      "require": "./devtools.js",
      "import": "./devtools.mjs"
    },
    "./subscribe": {
      "require": "./subscribe.js",
      "import": "./subscribe.mjs"
    },
    "./react": {
      "require": "./react.js",
      "import": "./react.mjs"
    },
    "./preact": {
      "require": "./preact.js",
      "import": "./preact.mjs"
    },
    "./vue": {
      "require": "./vue.js",
      "import": "./vue.mjs"
    },
    "./lit": {
      "require": "./lit.js",
      "import": "./lit.mjs"
    },
    "./rxjs": {
      "require": "./rxjs.js",
      "import": "./rxjs.mjs"
    },
    "./svelte": {
      "require": "./svelte.js",
      "import": "./svelte.mjs"
    }
  }
}
