    "./svelte": "./src/svelte.ts",
    "./solid": "./src/solid.ts",
    "./angular": "./src/angular.ts"
  },
  "imports": {
    "exome": "./src/exome.ts",
    "react": "npm:react@^18.2.0",
    "preact/hooks": "npm:preact@^10.19.6/hooks",
    "vue": "npm:vue@^3.4.21",
    "lit": "npm:lit@^3.1.2",
    "rxjs": "npm:rxjs@^7.8.1",
